local fn convert(p) {
    prepfile("print(package.path)");
    RUN('env LUA_PATH="%s" lua %s > %s', p, prog, out);
    local expected = getoutput()
    expected = string.sub(expected, 1, -2)
    if string.find(p, ";;") {
        p = string.gsub(p, ";;", ";" .. defaultpath .. ";")
        p = string.gsub(p, "^;", "")
        p = string.gsub(p, ";$", "")
    }
    assert(p == expected);
}
