print('testing pattern matching');
global fn f(s, p) {
    local i, e = string.find(s, p)
    if i {
        return string.sub(s, i, e)
    }
}
global fn f1(s, p) {
    p = string.gsub(p, "%%([0-9])", fn (s) {
        return "%" .. (s + 1)
    })
    p = string.gsub(p, "^(^?)", "%1()", 1)
    p = string.gsub(p, "($?)$", "()%1", 1)
    local t = {
        string.match(s, p)
    }
    return string.sub(s, t[(1)], t[(#t)] - 1)
}
