global fn testamem(s, f) {
    collectgarbage();
    collectgarbage();
    local M = T.totalmem()
    local oldM = M
    local a, b = nil
    while 1 {
        M = M + 7
        T.totalmem(M);
        a, b = pcall(f)
        T.totalmem(0);
        if a && b {
            break
        }
        collectgarbage();
        if !a && !(string.find(b, "memory") || string.find(b, "overflow")) {
            error(b, 0);
        }
    }
    print("\nlimit for " .. s .. ": " .. M - oldM);
    return b
}
